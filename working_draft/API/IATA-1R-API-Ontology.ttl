xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://onerecord.iata.org/Notification#changeRequest
<https://onerecord.iata.org/Notification#changeRequest> rdf:type owl:ObjectProperty ;
                                                        rdfs:comment "Optional URI to the ChangeRequest if the Notification#eventType is one of  CHANGEREQUEST_ACCEPTED, CHANGEREQUEST_REJECT, or CHANGEREQUEST_FAILED"@en ;
                                                        rdfs:label "notification:changeRequest"@en .


###  https://onerecord.iata.org/Notification#logisticsObjectRef
<https://onerecord.iata.org/Notification#logisticsObjectRef> rdf:type owl:ObjectProperty ;
                                                             rdfs:comment "Identifier of the LogisticsObject to which the Notification belongs to"@en ;
                                                             rdfs:label "notification:logisticsObjectRef"@en .


###  https://onerecord.iata.org/api/AuditTrail#changeRequests
auditTrail:changeRequests rdf:type owl:ObjectProperty ;
                          rdfs:domain :AuditTrail ;
                          rdfs:range :ChangeRequest ;
                          rdfs:comment "List of change requests that were sent as PATCH on for a Logistics Object"@en ;
                          rdfs:label "auditTrail:changeRequests"@en .


###  https://onerecord.iata.org/api/AuditTrail#logisticsObjectRef
auditTrail:logisticsObjectRef rdf:type owl:ObjectProperty ;
                              rdfs:domain :AuditTrail ;
                              rdfs:range :LogisticsObjectRef .


###  https://onerecord.iata.org/api/ChangeRequest#errors
changeRequest:errors rdf:type owl:ObjectProperty ;
                     rdfs:comment "Contains a list of Error objects if the patching of the LogisticsObject was not successful"@en ;
                     rdfs:label "changeRequest:errors"@en .


###  https://onerecord.iata.org/api/ChangeRequest#logisticsObjectRef
changeRequest:logisticsObjectRef rdf:type owl:ObjectProperty ;
                                 rdfs:domain :ChangeRequest ;
                                 rdfs:range :LogisticsObjectRef .


###  https://onerecord.iata.org/api/ChangeRequest#operations
changeRequest:operations rdf:type owl:ObjectProperty ;
                         rdfs:domain :ChangeRequest ;
                         rdfs:range :Operation ;
                         rdfs:comment "List of operations to apply as PATCH on a Logistics Object"@en ;
                         rdfs:label "changeRequest:operations"@en .


###  https://onerecord.iata.org/api/ChangeRequest#requestedBy
changeRequest:requestedBy rdf:type owl:ObjectProperty ;
                          rdfs:domain :ChangeRequest ;
                          rdfs:range :LogisticsObjectRef ;
                          rdfs:comment "Reference to the identifier that represents the party that has requested the change request"@en ;
                          rdfs:label "changeRequest:requestedBy"@en .


###  https://onerecord.iata.org/api/DelegationRequest#errors
delegationRequest:errors rdf:type owl:ObjectProperty ;
                         rdfs:label "Contains a list of Error objects if the processing of the DelegationRequest was not successful"@en ,
                                    "delegationRequest:errors"@en .


###  https://onerecord.iata.org/api/DelegationRequest#targetLogisticsObjects
delegationRequest:targetLogisticsObjects rdf:type owl:ObjectProperty ;
                                         rdfs:domain :DelegationRequest ;
                                         rdfs:range :LogisticsObjectRef ;
                                         rdfs:comment "Identifiers of the logistics objects to which the access is requested"@en ;
                                         rdfs:label "delegationRequest:targetLogisticsObjects"@en .


###  https://onerecord.iata.org/api/Error#details
error:details rdf:type owl:ObjectProperty ;
              rdfs:domain :Error ;
              rdfs:range :ErrorDetails ;
              rdfs:comment "Error details"@en ;
              rdfs:label "error:details"@en .


###  https://onerecord.iata.org/api/Operation#o
operation:o rdf:type owl:ObjectProperty ;
            rdfs:domain :Operation ;
            rdfs:range :OperationObject ;
            rdfs:comment "PATCH object to modify"@en ;
            rdfs:label "operation:o"@en .


###  https://onerecord.iata.org/api/ServerInformation#dataOwner
<https://onerecord.iata.org/api/ServerInformation#dataOwner> rdf:type owl:ObjectProperty ;
                                                             rdfs:domain :ServerInformation ;
                                                             rdfs:range :LogisticsObjectRef ;
                                                             rdfs:comment "Owner of the data hosted on the ONE Record server"@en ;
                                                             rdfs:label "serverInformation:dataOwner"@en .


###  https://onerecord.iata.org/api/Subscription#myCompanyIdentifier
subscription:myCompanyIdentifier rdf:type owl:ObjectProperty ;
                                 rdfs:domain :Subscription ;
                                 rdfs:range :LogisticsObjectRef ;
                                 rdfs:comment "The company identifier from the Internet of Logistics of my company."@en ;
                                 rdfs:label "subscription:myCompanyIdentifier"@en .


#################################################################
#    Data properties
#################################################################

###  https://onerecord.iata.org/DelegationRequest#status
<https://onerecord.iata.org/DelegationRequest#status> rdf:type owl:DatatypeProperty ;
                                                      rdfs:range [ rdf:type rdfs:Datatype ;
                                                                   owl:oneOf [ rdf:type rdf:List ;
                                                                               rdf:first "ACCEPTED" ;
                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                          rdf:first "PENDING" ;
                                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                                     rdf:first "REJECTED" ;
                                                                                                     rdf:rest rdf:nil
                                                                                                   ]
                                                                                        ]
                                                                             ]
                                                                 ] ;
                                                      rdfs:comment "PENDING, ACCEPTED or REJECTED" ;
                                                      rdfs:label "delegationRequest:status"@en .


###  https://onerecord.iata.org/ServerInformation#supportedEncodings
<https://onerecord.iata.org/ServerInformation#supportedEncodings> rdf:type owl:DatatypeProperty ;
                                                                  rdfs:comment "Optional list of supported encodings of the ONE Record server, e.g. gzip"@en ;
                                                                  rdfs:label "serverInformation:supportedEncodings"@en ;
                                                                  rdfs:seeAlso "https://developer.mozilla.org/en-US/docs/Web/HTTP/Headers/Accept-Encoding"@en .


###  https://onerecord.iata.org/ServerInformation#supportedLanguages
<https://onerecord.iata.org/ServerInformation#supportedLanguages> rdf:type owl:DatatypeProperty ;
                                                                  rdfs:comment "Supported languages of the ONE Record API, minimum is en (English)"@en ;
                                                                  rdfs:label "serverInformation:supportedLanguages"@en .


###  https://onerecord.iata.org/api/AuditTrail#latestRevision
auditTrail:latestRevision rdf:type owl:DatatypeProperty ;
                          rdfs:domain :AuditTrail ;
                          rdfs:range xsd:nonNegativeInteger ;
                          rdfs:comment "Latest revision of the Logistics Object. Starting with revision 0"@en ;
                          rdfs:label "auditTrail:latestRevision"@en .


###  https://onerecord.iata.org/api/AuditTrail#logisticsObjectRef
auditTrail:logisticsObjectRef rdf:type owl:DatatypeProperty .


###  https://onerecord.iata.org/api/ChangeRequest#callbackUrl
changeRequest:callbackUrl rdf:type owl:DatatypeProperty ;
                          rdfs:comment "Optional callback URL of the requester to be used for sending notifications about success, failure or rejection of the ChangeRequest"@en ;
                          rdfs:label "changeRequest:callbackUrl"@en .


###  https://onerecord.iata.org/api/ChangeRequest#description
changeRequest:description rdf:type owl:DatatypeProperty ;
                          rdfs:domain :ChangeRequest ;
                          rdfs:range xsd:string ;
                          rdfs:comment "Reason for the change (optional)"@en ;
                          rdfs:label "changeRequest:description"@en .


###  https://onerecord.iata.org/api/ChangeRequest#logisticsObjectRef
changeRequest:logisticsObjectRef rdf:type owl:DatatypeProperty .


###  https://onerecord.iata.org/api/ChangeRequest#requestedAt
changeRequest:requestedAt rdf:type owl:DatatypeProperty ;
                          rdfs:domain :ChangeRequest ;
                          rdfs:range xsd:dateTime ;
                          rdfs:comment "Datetime when the change request was created"@en ;
                          rdfs:label "changeRequest:requestedAt"@en .


###  https://onerecord.iata.org/api/ChangeRequest#revision
changeRequest:revision rdf:type owl:DatatypeProperty ;
                       rdfs:domain :ChangeRequest ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Revision number of the Logistics Object, starting with 0 for changing the initial revision of a Logistics Object"@en ;
                       rdfs:label "changeRequest:revision"@en .


###  https://onerecord.iata.org/api/ChangeRequest#status
changeRequest:status rdf:type owl:DatatypeProperty ;
                     rdfs:domain :ChangeRequest ;
                     rdfs:range [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "ACCEPTED" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "PENDING" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "REJECTED" ;
                                                                    rdf:rest rdf:nil
                                                                  ]
                                                       ]
                                            ]
                                ] ;
                     rdfs:comment "PENDING, ACCEPTED or REJECTED"@en ;
                     rdfs:label "changeRequest:status"@en .


###  https://onerecord.iata.org/api/DelegationRequest#action
delegationRequest:action rdf:type owl:DatatypeProperty ;
                         rdfs:domain :DelegationRequest ;
                         rdfs:range [ rdf:type rdfs:Datatype ;
                                      owl:oneOf [ rdf:type rdf:List ;
                                                  rdf:first "DELEGATE" ;
                                                  rdf:rest [ rdf:type rdf:List ;
                                                             rdf:first "REVOKE" ;
                                                             rdf:rest rdf:nil
                                                           ]
                                                ]
                                    ] ;
                         rdfs:comment "REVOKE or DELEGATE"@en ;
                         rdfs:label "delegationRequest:action"@en .


###  https://onerecord.iata.org/api/DelegationRequest#operations
delegationRequest:operations rdf:type owl:DatatypeProperty ;
                             rdfs:domain :DelegationRequest ;
                             rdfs:range [ rdf:type rdfs:Datatype ;
                                          owl:oneOf [ rdf:type rdf:List ;
                                                      rdf:first "GET" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "PATCH" ;
                                                                 rdf:rest rdf:nil
                                                               ]
                                                    ]
                                        ] ;
                             rdfs:comment "GET or PATCH"@en ;
                             rdfs:label "delegationRequest:operations"@en .


###  https://onerecord.iata.org/api/DelegationRequest#targetCompanies
delegationRequest:targetCompanies rdf:type owl:DatatypeProperty ;
                                  rdfs:domain :DelegationRequest ;
                                  rdfs:range xsd:string ;
                                  rdfs:comment "Parties that receive the delegated rights"@en ;
                                  rdfs:label "delegationRequest:targetCompanies"@en .


###  https://onerecord.iata.org/api/Error#title
error:title rdf:type owl:DatatypeProperty ;
            rdfs:domain :Error ;
            rdfs:range xsd:string ;
            rdfs:comment "Short summary of the error"@en ;
            rdfs:label "error:title"@en .


###  https://onerecord.iata.org/api/ErrorDetails#code
errorDetails:code rdf:type owl:DatatypeProperty ;
                  rdfs:domain :ErrorDetails ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Error code is a numeric or alphanumeric code that can be used to determine the source of the error and why it occured."@en ;
                  rdfs:label "errorDetails:code"@en .


###  https://onerecord.iata.org/api/ErrorDetails#message
errorDetails:message rdf:type owl:DatatypeProperty ;
                     rdfs:domain :ErrorDetails ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Message that describes the error"@en ;
                     rdfs:label "errorDetails:message"@en .


###  https://onerecord.iata.org/api/ErrorDetails#property
errorDetails:property rdf:type owl:DatatypeProperty ;
                      rdfs:domain :ErrorDetails ;
                      rdfs:range xsd:anyURI ;
                      rdfs:comment """Property of the object for which the error applies in IRI format, i.e.
https://onerecord.iata.org/cargo/Piece#volumetricWeight"""@en ;
                      rdfs:label "errorDetails:property"@en .


###  https://onerecord.iata.org/api/ErrorDetails#resource
errorDetails:resource rdf:type owl:DatatypeProperty ;
                      rdfs:domain :ErrorDetails ;
                      rdfs:range xsd:anyURI ;
                      rdfs:comment "URI of the object where the error occurred"@en ;
                      rdfs:label "errorDetails:resource"@en .


###  https://onerecord.iata.org/api/LogisticsObjectRef#logisticsObjectId
logisticsObjectRef:logisticsObjectId rdf:type owl:DatatypeProperty ;
                                     rdfs:domain :LogisticsObjectRef ;
                                     rdfs:range xsd:string ;
                                     rdfs:comment "Id of the reference Logistics Object"@en ;
                                     rdfs:label "logisticsObjectRef:logisticsObjectId"@en .


###  https://onerecord.iata.org/api/LogisticsObjectRef#logisticsObjectType
logisticsObjectRef:logisticsObjectType rdf:type owl:DatatypeProperty ;
                                       rdfs:domain :LogisticsObjectRef ;
                                       rdfs:range xsd:string ;
                                       rdfs:comment "Type of the reference Logistics Object"@en ;
                                       rdfs:label "logisticsObjectRef:logisticsObjectType"@en .


###  https://onerecord.iata.org/api/Notification#changedProperties
notification:changedProperties rdf:type owl:DatatypeProperty ;
                               rdfs:domain :Notification ;
                               rdfs:range xsd:anyURI ;
                               rdfs:comment "List of all changed properties as IRIs after a ChangeRequet was successfully applied, e.g. [https://onerecord.iata.org/cargo/Piece#volumetricWeight, https://onerecord.iata.org/cargo/Piece#goodsDescription]"@en ;
                               rdfs:label "notification:changedProperties"@en .


###  https://onerecord.iata.org/api/Notification#eventType
notification:eventType rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Notification ;
                       rdfs:range [ rdf:type rdfs:Datatype ;
                                    owl:oneOf [ rdf:type rdf:List ;
                                                rdf:first "CHANGEREQUEST_ACCEPTED" ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "CHANGEREQUEST_FAILED" ;
                                                           rdf:rest [ rdf:type rdf:List ;
                                                                      rdf:first "CHANGEREQUEST_PENDING" ;
                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                 rdf:first "CHANGEREQUEST_REJECTED" ;
                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                            rdf:first "EVENT_RECEIVED" ;
                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                       rdf:first "OBJECT_CREATED" ;
                                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                                  rdf:first "OBJECT_UPDATED" ;
                                                                                                                  rdf:rest rdf:nil
                                                                                                                ]
                                                                                                     ]
                                                                                          ]
                                                                               ]
                                                                    ]
                                                         ]
                                              ]
                                  ] ;
                       rdfs:comment "OBJECT_CREATED, OBJECT_UPDATED, EVENT_RECEIVED, CHANGEREQUEST_PENDING, CHANGEREQUEST_ACCEPTED, CHANGEREQUEST_REJECTED, CHANGEREQUEST_FAILED"@en ;
                       rdfs:label "notification:eventType"@en .


###  https://onerecord.iata.org/api/Notification#topic
notification:topic rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Notification ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Type of LogisticsObject or specific LogisticsObjectIdentifier to which the message belongs, e.g. https://onerecord.iata.org/Piece or https://1r.example.com/7f01363f-0c6a-4414-be48-d3692e219b91"@en ;
                   rdfs:label "notification:topic"@en .


###  https://onerecord.iata.org/api/Operation#op
operation:op rdf:type owl:DatatypeProperty ;
             rdfs:domain :Operation ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:oneOf [ rdf:type rdf:List ;
                                      rdf:first "ADD" ;
                                      rdf:rest [ rdf:type rdf:List ;
                                                 rdf:first "DEL" ;
                                                 rdf:rest rdf:nil
                                               ]
                                    ]
                        ] ;
             rdfs:comment "Operation objects must have exactly one op (operation) member; this value indicates which operation is to be performed. The value must be one of ADD or DEL; all other values result in an error"@en ;
             rdfs:label "operation:op"@en .


###  https://onerecord.iata.org/api/Operation#p
operation:p rdf:type owl:DatatypeProperty ;
            rdfs:domain :Operation ;
            rdfs:range xsd:anyURI ;
            rdfs:comment "Operations objects must have exactly one p, predicate, member. The value of this member must be an URI, e.g. https://onerecord.iata.org/cargo/Piece#goodsDescription"@en ;
            rdfs:label "operation:p"@en .


###  https://onerecord.iata.org/api/Operation#s
operation:s rdf:type owl:DatatypeProperty ;
            rdfs:comment "Operation objects MUST have exactly one \"s\", subject, member. The value of this member MUST be one of IRI or blank node."@en ;
            rdfs:label "operation:s"@en ;
            rdfs:seeAlso "https://github.com/digibib/ls.ext/wiki/JSON-LD-PATCH#operationse"@en .


###  https://onerecord.iata.org/api/OperationObject#datatype
operationObject:datatype rdf:type owl:DatatypeProperty ;
                         rdfs:domain :OperationObject ;
                         rdfs:range xsd:anyURI ;
                         rdfs:comment "Data type of the field to update, must be a valid URI, e.g. http://www.w3.org/2001/XMLSchema#int"@en ;
                         rdfs:label "operationObject:datatype"@en .


###  https://onerecord.iata.org/api/OperationObject#value
operationObject:value rdf:type owl:DatatypeProperty ;
                      rdfs:domain :OperationObject ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Updated value for the field"@en ;
                      rdfs:label "operationObject:value"@en .


###  https://onerecord.iata.org/api/ServerInformation#serverEndpoint
<https://onerecord.iata.org/api/ServerInformation#serverEndpoint> rdf:type owl:DatatypeProperty ;
                                                                  rdfs:domain :ServerInformation ;
                                                                  rdfs:range xsd:string ;
                                                                  rdfs:comment "ONE Record API endpoint in the Internet of Logistics"@en ;
                                                                  rdfs:label "serverInformation:serverEndpoint"@en .


###  https://onerecord.iata.org/api/ServerInformation#supportedContentTypes
<https://onerecord.iata.org/api/ServerInformation#supportedContentTypes> rdf:type owl:DatatypeProperty ;
                                                                         rdfs:domain :ServerInformation ;
                                                                         rdfs:range xsd:string ;
                                                                         rdfs:comment "Supported content types of the server, MUST contain at least application/ld+json"@en ;
                                                                         rdfs:label "serverInformation:supportedContentTypes"@en .


###  https://onerecord.iata.org/api/ServerInformation#supportedLogisticsObjectTypes
<https://onerecord.iata.org/api/ServerInformation#supportedLogisticsObjectTypes> rdf:type owl:DatatypeProperty ;
                                                                                 rdfs:domain :ServerInformation ;
                                                                                 rdfs:range xsd:anyURI ;
                                                                                 rdfs:comment "Supported logistics object types on the server"@en ;
                                                                                 rdfs:label "serverInformation:supportedLogisticsObjectTypes"@en .


###  https://onerecord.iata.org/api/Subscription#cacheFor
subscription:cacheFor rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Subscription ;
                      rdfs:range xsd:nonNegativeInteger ;
                      rdfs:comment "Duration of the period to cache the subscription information in seconds"@en ;
                      rdfs:label "subscription:cacheFor"@en .


###  https://onerecord.iata.org/api/Subscription#callbackUrl
subscription:callbackUrl rdf:type owl:DatatypeProperty ;
                         rdfs:domain :Subscription ;
                         rdfs:range xsd:string ;
                         rdfs:comment "Callback URL of the Client Subscription API where the subscriber receives Logistics Objects"@en ;
                         rdfs:label "subscription:callbackUrl"@en .


###  https://onerecord.iata.org/api/Subscription#contentTypes
subscription:contentTypes rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Subscription ;
                          rdfs:range xsd:string ;
                          rdfs:comment "Content types that the subscriber wants to receive in the notifications, e.g. application/ld+json"@en ;
                          rdfs:label "subscription:contentTypes"@en ;
                          rdfs:seeAlso "https://developer.mozilla.org/en-US/docs/Web/HTTP/Basics_of_HTTP/MIME_types/Common_types"@en .


###  https://onerecord.iata.org/api/Subscription#secret
subscription:secret rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Subscription ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Either a secret or API Key that ensures that only companies with this subscription information can POST to the subscriber callback endpoint"@en ;
                    rdfs:label "subscription:secret"@en .


###  https://onerecord.iata.org/api/Subscription#sendLogisticsObjectBody
subscription:sendLogisticsObjectBody rdf:type owl:DatatypeProperty ;
                                     rdfs:domain :Subscription ;
                                     rdfs:range xsd:boolean ;
                                     rdfs:comment "Flag specifying if the publisher should send the whole logistics object or not in the notification object"@en ;
                                     rdfs:label "subscription:sendLogisticsObjectBody"@en .


###  https://onerecord.iata.org/api/Subscription#subscribeToLogisticsEvents
subscription:subscribeToLogisticsEvents rdf:type owl:DatatypeProperty ;
                                        rdfs:domain :Subscription ;
                                        rdfs:range xsd:boolean ;
                                        rdfs:comment "Flag specifying if the subscriber wants to receive Notification from the publisher when LogisticsEvents for a Logistics Object are received, default=FALSE"@en ;
                                        rdfs:label "subscription:subscribeToLogisticsEvents"@en ;
                                        rdfs:seeAlso "https://onerecord.iata.org/cargo/LogisticsEvent"^^xsd:anyURI .


###  https://onerecord.iata.org/api/Subscription#subscribedTo
subscription:subscribedTo rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Subscription ;
                          rdfs:range xsd:anyURI ;
                          rdfs:comment "Company Identifier of the company the subscriber wants to subscribe to (delegation scenario)."@en ;
                          rdfs:label "subscription:subscribedTo"@en .


###  https://onerecord.iata.org/api/Subscription#topic
subscription:topic rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Subscription ;
                   rdfs:range xsd:anyURI ;
                   rdfs:comment "The Logistics Object type to which the subscriber wants subscribe to"@en ;
                   rdfs:label "subscription:topic"@en .


